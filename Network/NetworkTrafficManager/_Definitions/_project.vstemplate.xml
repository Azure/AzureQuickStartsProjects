<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
  <TemplateData>
    <Name>Traffic Manager</Name>
    <Description>This tutorial demonstrates how you can quickly create a Traffic Manager profile with a basic configuration using Quick Create in the Management Portal. Use Traffic Manager as part of your overall networking solution - Traffic Manager applies an intelligent policy engine to the DNS queries on your domain names so that you can send traffic to the best data center for performance, business continuity, price, compliance, legal, or tax purposes</Description>

    <DefaultName>TrafficManagerTutorial</DefaultName>
    
    <ProjectType>CSharp</ProjectType>
    <ProjectSubType></ProjectSubType>
    <SortOrder>1000</SortOrder>
    <CreateNewFolder>true</CreateNewFolder>    
    <ProvideDefaultName>true</ProvideDefaultName>
    <LocationField>Enabled</LocationField>
    <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
    <Icon>sw-file-icon.png</Icon>
    
    <!-- Indicates how many parent folders this item template should appear in -->
    <NumberOfParentCategoriesToRollUp>1</NumberOfParentCategoriesToRollUp>
  </TemplateData>
  <TemplateContent>
    <Project TargetFileName="NetworkTrafficManager.csproj" File="NetworkTrafficManager.csproj" ReplaceParameters="true">      
    </Project>
  </TemplateContent>
  <WizardExtension>
    <Assembly>ProjectWizard, Version=1.0.0.0, Culture=Neutral, PublicKeyToken=386c05013bfc2784</Assembly>
    <FullClassName>ProjectWizard.Wizard</FullClassName>
  </WizardExtension>
  <WizardData>
    <navigation>
      <navigate path="http://go.microsoft.com/fwlink/?LinkId=325286" generateProject="false" />
    </navigation>
  </WizardData>
</VSTemplate>