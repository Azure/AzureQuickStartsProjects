<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
  <TemplateData>
    <Name>Azure Cloud Services Tutorial</Name>
    <Description>In this tutorial learn how to create a Cloud Service project within Visual Studio.</Description>
    <DefaultName>ComputeCloudServices</DefaultName>
    <ProjectType>CSharp</ProjectType>
    <ProjectSubType></ProjectSubType>
    <SortOrder>1002</SortOrder>
    <CreateNewFolder>true</CreateNewFolder>    
    <ProvideDefaultName>true</ProvideDefaultName>
    <LocationField>Enabled</LocationField>
    <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
    <Icon>sw-file-icon.png</Icon>
    
    <!-- Indicates how many parent folders this item template should appear in -->
    <NumberOfParentCategoriesToRollUp>1</NumberOfParentCategoriesToRollUp>
  </TemplateData>
  <TemplateContent>
    <Project TargetFileName="ComputeCloudServices.csproj" File="ComputeCloudServices.csproj" ReplaceParameters="true">      
    </Project>
  </TemplateContent>
  <WizardExtension>
    <Assembly>ProjectWizard, Version=1.0.0.0, Culture=Neutral, PublicKeyToken=f30ae472f039a534</Assembly>
    <FullClassName>ProjectWizard.Wizard</FullClassName>
  </WizardExtension>
  <WizardData>
    <navigation>
      <navigate path="http://go.microsoft.com/fwlink/?LinkId=325256" generateProject="false" />
    </navigation>
  </WizardData>
</VSTemplate>